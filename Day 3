class Pattern {
public static void main(String[]args)
{
int n=11;
for(int i=0;i<n;i++)
{
    for(int j=0;j<n;j++)
{
if(i==0||i==6||j==1||j==n\2||j==n)
{
System.out.print("*");
}
else
{
System.out.print(" ");
}
}
System.out.println();
}
}
}



class Plane {
    void takeoff()
    {
        System.out.prinln("plane");
    }
    void fly()
    {
        System.out.println("plane is taking off");
    }
    void land(){
        System.out.println("plane is takes landing");
    }
}
