        System.out.println("plane is taking off");
    }
}
class AirPort{
    void permit(plane ref){
        ref.takeoff();
        ref.fly();
        ref.land();
    }
}
public class MethodOfInheritance{
    public static void main(String[]args){
        CargoPlane cp = new CargoPlane();
        PassengerPlane pp= new PassengerPlane();
    }
    }
}
    
