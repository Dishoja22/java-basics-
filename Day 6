package day6;
import java.util.ArrayList;
public class ExampleForLinkedList {

	public static void main(String[] args) {
		
	List l = new LinkedList();
	l.add("java");
	l.add(10);
	l.add(35,5);
	l.add(23);
	l.add(null);
	
	System.out.println("The List: " + i);
	
	for(Object i : i)
	{
	    System.out.println(i);
	}
	System.out.println(l.get(0));
	
	System.out.println(l.isEmpty());
	
	System.out.println("---------------------------------");
	
	LinkedList.ll = new LinkedList();
	ll.add(10);
	ll.add(20);
	ll.add("cb");
	
	System.out.println("The LinkedList iss :" +ll);
	
	for(Object i :l)
	{
	    System.out.println(i);
	}
	System.out.println(l.get(0));
	
	
	
	
	-------> LINKEDLIST EXAMPLES




import java.util.LinkedList;

public class ExampleForLinkedList {
    public static void main(String[] args) {

        Employee e1 = new Employee("v", "t", "v@mail.com");
        Employee e2 = new Employee("s", "ss", "s@mail.com");
        Employee e3 = new Employee("d", "sds", "sds@mail.com");

        LinkedList<Employee> ll = new LinkedList<>();
        ll.add(e1);
        ll.add(e2);
        ll.add(e3);

        for (Employee emp : ll) {
            System.out.println(emp);
        }
    }
}

class Employee {
    private String empName;
    private String empDestination;
    private String empEmail;

    // Constructor
    public Employee(String empName, String empDestination, String empEmail) {
        this.empName = empName;
        this.empDestination = empDestination;
        this.empEmail = empEmail;
    }

    // Getters
    public String getEmpName() {
        return empName;
    }

    public String getEmpDestination() {
        return empDestination;
    }

    public String getEmpEmail() {
        return empEmail;
    }

    // toString method
    @Override
    public String toString() {
        return "Employee [empName=" + empName +
               ", empDestination=" + empDestination +
               ", empEmail=" + empEmail + "]";
    }
}




