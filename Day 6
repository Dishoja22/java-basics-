public class ExampleForHashSet {
    
    public static void main(String[]args) {
        Dog d = new Dog("buddy","lab");
        
        HashSet hs = new HashSet();
        hs.add(d);
        
        for(Object e : hs)
        {
            System.out.println(e);
        }
    }
}
