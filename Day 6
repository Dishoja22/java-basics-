----->VECTOR

import java.util.Vector;

public class ExampleForVector{
     public static void main(String[]args){
         
         Vector<String> v = new Vector();
         
         v.add("JAVA SCRIPT");
         v.add("php");
         v.add("dart");
         v.add("JAVA");
         
         System.out.println(v);
         
         System.out.println("-------------------------");
         
         System.out.println(v.get(2));
         
         System.out.println("--------------------------");
         
         System.out.println(v.isEmpty());
         
         System.out.println("--------------------------");
         
         System.out.println(v.contains(2));
         
         System.out.println("---------------------------");
         
         System.out.println(v.size());
         
         System.out.println("--------------------------");
         
         for(Object i : v)
         {
             System.out.println(i);
         }
         
     }
}


import java.util.Vector;

public class ExampleForVector{
     public static void main(String[]args){
         
         Vector<String> v = new Vector();
         
         v.add("JAVA SCRIPT");
         v.add("php");
         v.add("dart");
         v.add("JAVA");
         
         System.out.println(v);
         
         System.out.println("-------------------------");
         
         System.out.println(v.get(2));
         
         System.out.println("--------------------------");
         
         System.out.println(v.isEmpty());
         
         System.out.println("--------------------------");
         
         System.out.println(v.contains(2));
         
         System.out.println("---------------------------");
         
         System.out.println(v.size());
         
         System.out.println("--------------------------");
         
         for(Object i : v)
         {
             System.out.println(i);
         }
         
     }
}




---->VECTOR EXAMPLE

import java.util.Vector;

class Car {
    private String brand;
    private String model;
    private int year;

    // Constructor
    public Car(String brand, String model, int year) {
        this.brand = brand;
        this.model = model;
        this.year = year;
    }

    // toString method
    @Override
    public String toString() {
        return "Car [brand=" + brand + ", model=" + model + ", year=" + year + "]";
    }
}

public class ExampleForVector {
    public static void main(String[] args) {

        Vector<Car> v = new Vector<>();

        Car c1 = new Car("Toyota", "Camry", 2020);
        Car c2 = new Car("Honda", "Civic", 2019);
        Car c3 = new Car("Tesla", "Model 3", 2022);

        v.add(c1);
        v.add(c2);
        v.add(c3);

        // Print all cars
        System.out.println(v);

        System.out.println("-------------------------");

        // Get element at index 1
        System.out.println(v.get(1));

        System.out.println("--------------------------");

        // Check if vector is empty
        System.out.println(v.isEmpty());

        System.out.println("--------------------------");

        // Check if vector contains an object
        System.out.println(v.contains(c2));

        System.out.println("---------------------------");

        // Size of vector
        System.out.println(v.size());

        System.out.println("--------------------------");

        // Iterate using for-each
        for (Car car : v) {
            System.out.println(car);
        }
    }
}




----> SETS AND ITS TYPES





    /* Set:
    -> Pre-defined interface-->java.util
    -> jdk 1.2
    -> implementation classes --> HashSet,LinkedHashSet,TressSet
    Specifications:
    
    -> Insertion order will not be maintained
    -> Duplication are not allowed
    -> Not IndexBased
    -> Null values can be Stored
    
    1. HashSet:
    --------------------
    -> its a pre-Defined class which is present inside java.util-->jdk 1.2
    -> UnderLined DataStructure --> HashTable
    -> initial capacity-->16
    -> Fill ratio/ load factor --> 75%--> 16+12
    
    */
    import java.util.*;
    public class ExampleForSet{
        public static void main(String[]args){
            /*Set s = new HashSet();
            Set s = new LinkedHashSet();
            Set s = new TreeSet();*/
            
            HashSet<Object> hs = new HashSet();
            hs.add(10);
            hs.add("java");
            hs.add("python");
            
            System.out.println(hs);
            
            System.out.println("-------------");
            
            System.out.println(hs.size());
            
            System.out.println("-------------");
            
            System.out.println(hs.isEmpty());
            
            System.out.println("---------------");
            
            System.out.println(hs.clone());
            
            System.out.println("---------------");
            
            for(Object i : hs)
            {
                
                System.out.println(i);
            }
            
    
        
    }
}



